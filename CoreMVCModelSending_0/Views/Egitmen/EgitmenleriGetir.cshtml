@model List<Egitmen>

    <table class="table table-hover">
        <thead>
            <tr>
                <th>Isim</th>
                <th>SoyIsim</th>
            </tr>
        </thead>

        <tbody>
        @foreach (Egitmen item in Model)
        {
            <tr>
                <td>@item.Isim</td>
                <td>@item.SoyIsim</td>
            </tr>
        }
    </tbody>
    </table>
